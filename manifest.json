{
  "manifest_version": 3,
  "name": "Huntflow Menu Organizer",
  "version": "1.2",
  "description": "Группировка и сортировка вакансий по категориям в меню Huntflow.",
  "permissions": ["storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
